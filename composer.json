{
    "name": "dejwcake/craftable-translatable",
    "description": "Make your Eloquent models and other content translated into multiple languages",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "translations",
        "eloquent",
        "language",
        "locale",
        "translate",
        "localization",
        "admin",
        "cms",
        "laravel"
    ],
    "authors": [
        {
            "name": "Pavol Perdík",
            "email": "pavol.perdik@brackets.sk",
            "homepage": "https://www.brackets.sk",
            "role": "Developer"
        },
        {
            "name": "David Běhal",
            "email": "david.behal@brackets.sk",
            "homepage": "https://www.brackets.sk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "laravel/framework": "^10.0",
        "spatie/laravel-translatable": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "~8.5",
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "Brackets\\Translatable\\": "src/"
        },
        "classmap": []
    },
    "autoload-dev": {
        "psr-4": {
            "Brackets\\Translatable\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brackets\\Translatable\\TranslatableServiceProvider"
            ]
        }
    },
    "branch-alias": {
        "dev-master": "1.0"
    }
}
